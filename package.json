{
    "name"            : "svg-reactify",
    "description"     : "transform SVG files into React elements",
    "homepage"        : "https://github.com/coma/svg-reactify",
    "repository"      : {
        "type": "git",
        "url" : "git://github.com/coma/svg-reactify.git"
    },
    "version"         : "2.0.0-alpha.2",
    "main"            : "lib",
    "license"         : "MIT",
    "private"         : false,
    "author"          : "Eduardo García Sanz <eduardo@comakai.com>",
    "contributors"    : [
        "Matthew Brandly <matt@brandly.me>",
        "Anders Stalheim Øfsdahl <https://github.com/andersaloof>"
    ],
    "keywords"        : [
        "browserify",
        "react",
        "reactjs",
        "svg",
        "browserify-transform"
    ],
    "dependencies"    : {
        "babel-core"         : "6.6",
        "babel-preset-react" : "6.5",
        "lodash.camelcase"   : "4.1",
        "lodash.kebabcase"   : "4.0",
        "svgo"               : "0.6",
        "through2"           : "2.0"
    },
    "peerDependencies": {
        "react": "15.0"
    },
    "devDependencies" : {
        "babel-cli"              : "6.5",
        "babel-eslint"           : "5.0",
        "babel-preset-es2015"    : "6.5",
        "enzyme"                 : "2.0",
        "eslint"                 : "2.2",
        "react"                  : "15.0",
        "react-addons-test-utils": "15.0",
        "react-dom"              : "15.0",
        "tape"                   : "4.4"
    },
    "scripts"         : {
        "clean"     : "rm -rf ./lib",
        "lint"      : "eslint src",
        "build"     : "npm run clean && babel src --out-dir lib",
        "test"      : "npm run lint && babel-node test",
        "prepublish": "npm test && npm run build"
    },
    "engines"         : {
        "node": ">= 4.0.0"
    }
}
